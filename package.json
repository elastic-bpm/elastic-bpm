{
  "name": "elastic-dashboard",
  "version": "1.0.0",
  "description": "Dashboard for the Elastic BPM implementation",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node app.js",
    "start:local": "set DEBUG=express:* & nodemon app.js",
    "wait": "timeout 3",
    "docker:reload": "npm run docker:kill && npm run docker:build && npm run wait && npm run docker:run",
    "docker:kill": "docker rm elastic-dashboard --force || echo 'ok'",
    "docker:build": "docker build -t elastic-dashboard .",
    "docker:run": "docker run --name elastic-dashboard -d -p 3000:3000 elastic-dashboard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DJBnjack/elastic-dashboard.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DJBnjack/elastic-dashboard/issues"
  },
  "homepage": "https://github.com/DJBnjack/elastic-dashboard#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0"
  }
}
