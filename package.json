{
  "name": "elastic-api",
  "version": "1.0.0",
  "description": "- name\r - owner\r - created\r - definition",
  "main": "app.js",
  "scripts": {
    "test": "mocha tests --recursive --",
    "test:watch": "mocha tests --recursive --watch",
    "start": "npm run docker:reload && npm run start:watch",
    "docker:reload": "npm run docker:kill && npm run docker:wait && npm run docker:start",
    "docker:kill": "docker rm some-redis --force || echo 'ok'",
    "docker:wait": "timeout 3",
    "docker:start": "docker run --name some-redis -d -p 6379:6379 redis",
    "start:watch": "set DEBUG=express:* & nodemon app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DJBnjack/elastic-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DJBnjack/elastic-api/issues"
  },
  "homepage": "https://github.com/DJBnjack/elastic-api#readme",
  "devDependencies": {
    "nodemon": "latest",
    "mocha": "latest",
    "chai": "latest",
    "proxyquire": "latest"
  },
  "dependencies": {
    "body-parser": "latest",
    "express": "latest",
    "redis": "latest"
  }
}
